{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "interospect:prisma": "prisma introspect",
    "predeploy": "run-s generate:nexus",
    "deploy": "corss-env NODE_ENV=production sls deploy",
    "watch:nexus": "src/schema",
    "offline": "sls offline",
    "dev": "run-p generate:prisma watch:nexus offline",
    "db:save": "dotenv -- prisma migrate save --experimental",
    "db:migrate": "dotenv -- prisma migrate up --experimental"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.5.0",
    "@nexus/schema": "0.17.0",
    "@prisma/client": "2.10.0",
    "PubSub": "^3.6.2",
    "apollo-server-lambda": "^2.19.1",
    "bcryptjs": "2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-middleware": "4.0.1",
    "graphql-shield": "7.4.0",
    "jsonwebtoken": "8.5.1",
    "nexus-plugin-prisma": "0.23.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@prisma/cli": "2.10.1",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.19.3",
    "@types/ws": "7.2.9",
    "serverless": "^2.16.1",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^5.3.5",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0-pre.50",
    "webpack": "^5.11.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
